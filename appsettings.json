{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning"
    },
    "ConsoleThemeType": "Ansi",
    "Console": {
      "FormatterName": "json",
      "FormatterOptions": {
        "IncludeScopes": true,
        "TimestampFormat": "u",
        "UseUtcTimestamp": true
      }
    }
  },
  "HomeAssistant": {
    "Host": "homeassistant.local",
    "Port": 8123,
    "Ssl": false,
  },
    "NetDaemon": {
        "ApplicationConfigurationFolder": "./apps"
    },
  "CodeGeneration": {
    "Namespace": "HomeAssistantGenerated",
    "OutputFile": "HomeAssistantGenerated.cs",
    "UseAttributeBaseClasses": "false"
  },
  
}
